Cell[BoxData[
 RowBox[{
  RowBox[{"\[Psi]", "[", 
   RowBox[{"x1_", ",", "x2_"}], "]"}], "=", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"x1", "-", "x2"}], ")"}], 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[Gamma]", 
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["x1", "2"], "+", 
            SuperscriptBox["x2", "2"]}], ")"}]}], "+", 
         RowBox[{"\[Alpha]", " ", "x1", " ", "x2"}]}], ")"}]}], "]"}]}], "/.", 
    RowBox[{"{", 
     RowBox[{"\[Alpha]", "\[Rule]", 
      RowBox[{"2", 
       RowBox[{"(", 
        RowBox[{"1", "-", "\[Gamma]"}], ")"}]}]}], "}"}]}], "//", 
   "FullSimplify"}]}]], "Input",
 CellChangeTimes->{{3.679747765950732*^9, 3.6797477860485363`*^9}, {
  3.67974804449857*^9, 3.6797480568095284`*^9}, {3.679748181675006*^9, 
  3.679748187274221*^9}}],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "x1"}], " ", "x2"}], "-", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"x1", "-", "x2"}], ")"}], "2"], " ", "\[Gamma]"}]}]], " ", 
  RowBox[{"(", 
   RowBox[{"x1", "-", "x2"}], ")"}]}]], "Output",
 CellChangeTimes->{3.679747845018433*^9, 3.6797480590277023`*^9, 
  3.679748188291239*^9, 3.679915748190535*^9}]
}, Open  ]],


Cell["Exact solution for two-particle system", "Section",
 CellChangeTimes->{{3.674486559661519*^9, 3.674486652324586*^9}, {
   3.675528061986125*^9, 3.675528067152973*^9}, {3.675877797090274*^9, 
   3.6758778288830433`*^9}, {3.6763114203326283`*^9, 
   3.6763114287545643`*^9}, {3.6766706992835903`*^9, 3.6766707114726686`*^9}, 
   3.676718921937498*^9, 3.6797496548006372`*^9}],

Cell[TextData[{
 "The Schrodinger Equation for two particles is\n\n\t",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{
        FractionBox[
         SuperscriptBox["\[PartialD]", "2"], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox[
           SubscriptBox["s", "1"], "2"]}]], 
        RowBox[{"-", 
         RowBox[{
          FractionBox[
           SuperscriptBox["\[PartialD]", "2"], 
           RowBox[{"\[PartialD]", 
            SuperscriptBox[
             SubscriptBox["s", "2"], "2"]}]], 
          RowBox[{"+", 
           SuperscriptBox[
            SubscriptBox["s", "1"], "2"]}]}]}]}]}], "+", 
      SuperscriptBox[
       SubscriptBox["s", "2"], "2"], "-", 
      RowBox[{
       FractionBox["1", "2"], "\[Beta]", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["s", "1"], "-", 
          SubscriptBox["s", "2"]}], ")"}], "2"]}]}], ")"}], 
    RowBox[{"\[Psi]", "[", 
     RowBox[{
      SubscriptBox["s", "1"], ",", 
      SubscriptBox["s", "2"]}], "]"}]}], "=", 
   RowBox[{"E\[Psi]", "[", 
    RowBox[{
     SubscriptBox["s", "1"], ",", 
     SubscriptBox["s", "2"]}], "]"}]}]],
  CellChangeTimes->{{3.67630649766282*^9, 3.676306539855228*^9}, {
    3.6763070484400797`*^9, 3.6763070502650146`*^9}, 3.676307387887595*^9, 
    3.676311510161159*^9, {3.676311540939328*^9, 3.676311545770981*^9}, {
    3.676387475956471*^9, 3.67638748009764*^9}}],
 "\nThe wavefunction solution to this equation is\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Psi]", "(", 
     RowBox[{
      SubscriptBox["x", "1"], ",", 
      SubscriptBox["x", "2"]}], ")"}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["x", "1"], "-", 
       SubscriptBox["x", "2"]}], ")"}], 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[Gamma]", "(", 
          RowBox[{
           SuperscriptBox[
            SubscriptBox["x", "1"], "2"], "+", 
           SuperscriptBox[
            SubscriptBox["x", "2"], "2"]}], ")"}], "+", 
         RowBox[{"\[Alpha]", " ", 
          SubscriptBox["x", "1"], " ", 
          SubscriptBox["x", "2"]}]}], ")"}]}], "]"}]}]}], TraditionalForm]]],
 "\nwith\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Gamma]", "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       SqrtBox[
        RowBox[{"1", "-", "\[Beta]"}]]}], ")"}]}]}], TraditionalForm]]],
 "\tand\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    RowBox[{"1", "-", 
     SqrtBox[
      RowBox[{"1", "-", "\[Beta]"}]]}]}], TraditionalForm]]],
 "\t\twith\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"E", "=", 
    RowBox[{"1", "+", 
     RowBox[{"3", " ", 
      SqrtBox[
       RowBox[{"1", "-", "\[Beta]"}]]}]}]}], TraditionalForm]]],
 "\nNote that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Gamma]", "+", 
     RowBox[{
      FractionBox["1", "2"], "\[Alpha]"}]}], "=", 
    RowBox[{
     RowBox[{"1", " ", "\[Rule]", " ", "\[Alpha]"}], "=", 
     RowBox[{"2", 
      RowBox[{"(", 
       RowBox[{"1", "-", "\[Gamma]"}], ")"}]}]}]}], TraditionalForm]]],
 ".  This is not a general result (for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"N", " ", "\[NotEqual]", " ", "2"}], ")"}], TraditionalForm]]],
 ".  Note also that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", "2"], " ", "<", " ", "\[Gamma]", " ", "\[LessEqual]", 
    " ", "1"}], TraditionalForm]]]
}], "Text",
